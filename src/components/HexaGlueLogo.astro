---
interface Props {
	loading?: 'eager' | 'lazy';
}

const { loading = 'eager', ...attrs } = Astro.props;
---

<div class="flex items-center gap-2" {...attrs}>
	<img src="/favicon.svg" width="32" height="32" alt="" aria-hidden="true" loading={loading} />
	<span class="font-heading text-white text-2xl font-extrabold">HexaGlue</span>
</div>
